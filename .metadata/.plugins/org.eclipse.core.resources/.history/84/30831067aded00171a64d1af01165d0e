import java.io.IOException;

public class abc {
	Date date = new Date();
	  
	String strDateFormat = "dd/MM/yyyy";
	  
	SimpleDateFormat simpleDateFormat = new SimpleDateFormat(strDateFormat);
	System.out.println("Hôm nay là ngày : " + simpleDateFormat.format(date));
}
